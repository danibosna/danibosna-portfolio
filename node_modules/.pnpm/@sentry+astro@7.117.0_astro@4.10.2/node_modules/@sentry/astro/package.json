{
  "name": "@sentry/astro",
  "version": "7.117.0",
  "description": "Official Sentry SDK for Astro",
  "repository": "git://github.com/getsentry/sentry-javascript.git",
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/astro",
  "keywords": [
    "withastro",
    "astro-component",
    "astro-integration",
    "sentry",
    "apm"
  ],
  "author": "Sentry",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.1"
  },
  "type": "module",
  "files": [
    "cjs",
    "esm",
    "types",
    "types-ts3.8"
  ],
  "main": "cjs/index.client.js",
  "module": "esm/index.server.js",
  "browser": "esm/index.client.js",
  "types": "types/index.types.d.ts",
  "exports": {
    ".": {
      "node": "./esm/index.server.js",
      "browser": "./esm/index.client.js",
      "import": "./esm/index.client.js",
      "require": "./cjs/index.server.js",
      "types": "./types/index.types.d.ts"
    },
    "./middleware": {
      "node": "./esm/integration/middleware/index.js",
      "import": "./esm/integration/middleware/index.js",
      "require": "./cjs/integration/middleware/index.js",
      "types": "./types/integration/middleware/index.types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "tag": "v7"
  },
  "peerDependencies": {
    "astro": ">=3.x || >=4.0.0-beta"
  },
  "dependencies": {
    "@sentry/browser": "7.117.0",
    "@sentry/core": "7.117.0",
    "@sentry/node": "7.117.0",
    "@sentry/types": "7.117.0",
    "@sentry/utils": "7.117.0",
    "@sentry/vite-plugin": "^2.8.0"
  },
  "devDependencies": {
    "astro": "^3.5.0",
    "rollup": "^3.20.2",
    "vite": "4.0.5"
  },
  "astro": {
    "external": true
  }
}